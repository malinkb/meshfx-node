
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '6'
	option macaddr '64:66:B3:DE:E1:A4'
	list ht_capab 'LDPC'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'

config wifi-iface 'wmesh0'
	option device 'radio0'
	option ifname 'adhoc0'
	option encryption 'none'
	option mode 'adhoc'
	option bssid '02:BA:00:00:00:01'
	option ssid 'dugnadsnett.no'

config wifi-iface 'accesspoint0'
	option device 'radio0'
	option ifname 'wifiap0'
	option network 'dugnadsnett'
	option mode 'ap'
	option ssid 'openwireless.org'
	option encryption 'none'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '36'
	option macaddr '64:66:B3:DE:E1:A5'
	list ht_capab 'LDPC'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option txpower '27'
	option country 'NO'

config wifi-iface 'wmesh1'
	option device 'radio1'
	option ifname 'adhoc1'
	option encryption 'none'
	option mode 'adhoc'
	option bssid '02:BA:00:00:00:01'
	option ssid 'dugnadsnett.no'

config wifi-iface 'accesspoint1'
	option device 'radio1'
	option ifname 'wifiap1'
	option network 'dugnadsnett'
	option mode 'ap'
	option ssid 'openwireless.org'
	option encryption 'none'

config wifi-device 'radio2'
	option type 'mac80211'
	option channel '36'
	option hwmode '11na'
	option path 'ffe09000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option htmode 'HT20'
	list ht_capab 'LDPC'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'

config wifi-iface
	option device 'radio2'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

config wifi-device 'radio3'
	option type 'mac80211'
	option channel '11'
	option hwmode '11ng'
	option path 'ffe0a000.pcie/pci0001:02/0001:02:00.0/0001:03:00.0'
	option htmode 'HT20'
	list ht_capab 'LDPC'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'

config wifi-iface
	option device 'radio3'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

config wifi-iface
	option device 'radio1'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'
	option disabled '1'

